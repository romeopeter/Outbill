{
  "name": "Outbill",
  "description": "Easy invoice for small businesses.",
  "version": "1.0",
  "manifest_version": 3,
  "action": {
    "default_popup": "index.html",
    "default_icon": "sample-icon.png"
  },
  "background": {
    "service_worker": "/background-tasks/background.js"
  },
  "permissions": [
    "activeTab",
    "storage",
    "tabs"
  ]
}